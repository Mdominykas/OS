@startuml
repeat
  :1. Blokavimasis laukiant resurso “Vartotojo atmintis”;
  :2. Sukuriama puslapių lentelė;
  :3. Atlaisvinamas resursas "Pakrovimo paketas";
  :4. Blokavimasis laukiant "Iš Loader" resurso;
  :5. Atlaisvinamas resursas "supervizorinė atmintis";
  :6. Proceso "Virtuali mašina" kūrimas;
  repeat
    :7. Blokavimasis laukiant resurso "Iš Interrupt";
    :8. Proceso "Virtuali mašina" stabdymas;
    if (9 Įvedimo - išvedimo ar darbas su failais?) is (Taip) then
      if (10. Skaitymas iš failų) is (Taip) then
         :11. Atlaisvinti resursą "Darbas su failais";
         :12. Blokavimasis laukiant resurso "Darbo su failais pabaiga";
      else (Ne)
        if (13. Įvedimas?) is (Taip) then
          :14. Išvesti nurodytą vartotojui;
        else (Ne)
          :15. Blokavimasis laukiant "Vartotojo įvedimas" resurso, skirto šiam JobGovernor;
        endif
      endif
      :16. Proceso "Virtuali mašina" aktyvavimas;
    else (Ne)
      :17. Naikinti procesą "Virtuali mašina";
      :18. Atlaisvinti resursą "Vartotojo atmintis";
      :19. Blokavimasis laukiant "Neegzistuojantis" resurso;
      kill
    endif
@enduml
