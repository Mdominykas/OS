@startuml
repeat
    :1. Blokavimasis laukiant “Iš vartotojo sąsajos” resurso;
    if (2. Yra procesų laukiančių resurso "Vartotojo įvedimas") is (Taip) then
     :3. atlaisvinti resursą "Vartotojo įvedimas";
   else (Ne)
    switch (Įvedimo tipas?)
    case (shutdown)
      :4. Atlaisvinti resursą "MOS pabaiga";
    case (load prog)
      :5. Blokavimasis laukiant resurso "supervizorinė atmintis";
      :6. Blokų kopijavimas į supervizorinę atmintį;
      :7. Atlaisvinamas resursas “Užduotis supervizorinėje atmintyje”;
    endswitch
    endif
@enduml
